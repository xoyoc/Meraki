{
    "builds": [
        {
            "src": "sistema/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },    
        {
            "src": "/(.*)",
            "dest": "sistema/wsgi.py"
        }
    ],
    "commands": [
        "python manage.py collectstatic --noinput"
    ]
}